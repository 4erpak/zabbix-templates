                            <valuemap/>
                            <logtimefmt/>
                            <preprocessing/>
                            <jmx_endpoint/>
                            <application_prototypes/>
                            <master_item_prototype/>
                        </item_prototype>
                    </item_prototypes>
                    <trigger_prototypes>
                        <trigger_prototype>
                            <expression>{$CHECK_SSL_DOMAIN_NAMES}&lt;&gt;0 and {Template SSL checks:ssl[isDomainValid, {#DOMAIN}].last()}&lt;&gt;1</expression>
                            <recovery_mode>0</recovery_mode>
                            <recovery_expression/>
                            <name>Invalid SSL certificate for {#DOMAIN}</name>
                            <correlation_mode>0</correlation_mode>
                            <correlation_tag/>
                            <url/>
                            <status>0</status>
                            <priority>2</priority>
                            <description>The domain name {#DOMAIN} is not present in allowed domain names list. Try https://{#DOMAIN}</description>
                            <type>0</type>
                            <manual_close>1</manual_close>
                            <dependencies/>
                            <tags/>
                        </trigger_prototype>
                        <trigger_prototype>
                            <expression>{Template SSL checks:ssl[daysBeforeExpiration, {#DOMAIN}].last()}&lt;{$SSL_EXPIRATION_WARNING_DAYS}</expression>
                            <recovery_mode>0</recovery_mode>
                            <recovery_expression/>
