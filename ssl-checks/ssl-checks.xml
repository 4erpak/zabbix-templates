                            <correlation_mode>0</correlation_mode>
                            <correlation_tag/>
                            <url/>
                            <status>0</status>
                            <priority>2</priority>
                            <description/>
                            <type>0</type>
                            <manual_close>1</manual_close>
                            <dependencies/>
                            <tags/>
                        </trigger_prototype>
                    </trigger_prototypes>
                    <graph_prototypes/>
                    <host_prototypes/>
                    <jmx_endpoint/>
                </discovery_rule>
            </discovery_rules>
            <httptests/>
            <macros>
                <macro>
                    <macro>{$CHECK_SSL_DOMAIN_NAMES}</macro>
                    <value>1</value>
                </macro>
                <macro>
                    <macro>{$IGNORED_SSL_DOMAINS}</macro>
                    <value/>
                </macro>
                <macro>
                    <macro>{$SSL_EXPIRATION_WARNING_DAYS}</macro>
                    <value>7</value>
                </macro>
            </macros>
            <templates/>
            <screens/>
        </template>
