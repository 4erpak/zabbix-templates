    <templates>
        <template>
            <template>Template SSL checks</template>
            <name>Template SSL checks</name>
            <description/>
            <groups>
                <group>
                    <name>Templates</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>SSL</name>
                </application>
            </applications>
            <items/>
            <discovery_rules>
                <discovery_rule>
                    <name>SSL certificates check</name>
                    <type>0</type>
                    <snmp_community/>
                    <snmp_oid/>
                    <key>ssl.discovery[&quot;{$IGNORED_SSL_DOMAINS}&quot;]</key>
                    <delay>1h</delay>
                    <status>0</status>
                    <allowed_hosts/>
                    <snmpv3_contextname/>
